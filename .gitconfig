[push] 
        autoSetupRemote = true 
 
 
[alias] 
        au = !git add -u && git status 
        unstage = !git restore --staged "$@" && git status
        cm = commit -m 
        ca = commit --amend -m 
        cane = commit --amend --no-edit 
        co = checkout 
        cb = checkout -b 
        s = status 
        br = branch 
        brd = branch -d 